<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjetCesiXamarin.Pages.TableauDeBordPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Ressources favorites"
                    HorizontalOptions="FillAndExpand" Command="{Binding NavigateToRessourcesFavoritesPageCommand}"/>
            <Button Text="Ressources exploitées"
                    HorizontalOptions="FillAndExpand" Command="{Binding NavigateToRessourcesExploitePageCommand}"/>
            <Button Text="Ressources mises de côté"
                    HorizontalOptions="FillAndExpand" Command="{Binding NavigateToRessourcesMisCotePageCommand}"/>
            <Button Text="Ressources créées"
                    HorizontalOptions="FillAndExpand" Command="{Binding NavigateToRessourcesCreesPageCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>